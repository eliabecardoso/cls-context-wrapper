{
  "name": "@ehsc/cls-context-wrapper",
  "version": "1.0.7",
  "description": "A wrapper of the cls-hooked and node:asynchooks libs to use as middleware",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --runInBand --config jestconfig.js",
    "dev": "node index",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "engineStrict": false,
  "engines": {
    "node": "^4.7 || >=6.9 || >=7.3 || >=8.2.1"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eliabecardoso/cls-context-wrapper"
  },
  "author": {
    "name": "Eliabe Henrique S. Cardoso",
    "email": "eliabe.hc@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "cls context wrapper",
    "cls-hooked",
    "cls hooked wrapper",
    "context",
    "async_hooks",
    "AsyncLocalStorage",
    "continuous local storage wrapper"
  ],
  "dependencies": {
    "@ehsc/cls-hooked": "^1.0.0",
    "semver": "^7.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/semver": "^7.3.12",
    "@types/uuid": "^8.3.4",
    "express": "^4.18.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  }
}
