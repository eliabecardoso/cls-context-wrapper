{
  "name": "@eliabecardoso/cls-context-wrapper",
  "version": "1.0.0",
  "description": "A wrapper of the cls-hooked lib to use as a middleware",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "dev": "node index",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "url": "@"
  },
  "author": "Eliabe Henrique dos Santos Cardoso",
  "license": "ISC",
  "keywords": [
    "cls context wrapper",
    "cls-hooked",
    "cls hooked wrapper",
    "context",
    "async_hooks",
    "AsyncLocalStorage",
    "continuous local storage wrapper"
  ],
  "dependencies": {
    "@eliabecardoso/cls-hooked": "^1.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/uuid": "^8.3.4",
    "express": "^4.18.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  }
}
